@page "/{id:int?}"@model Cosa_Cristina_Proiect.Pages.Reincarcari.IndexModel@{    ViewData["Title"] = "Index";}<h2>Istoric Reincarcari</h2><p>    <a asp-page="Create">Create New</a></p><table class="table">    <thead>        <tr>            <th>                @Html.DisplayNameFor(model => model.Reincarcare[0].Numar)            </th>            <th>                @Html.DisplayNameFor(model => model.Reincarcare[0].Data)            </th>            <th>                @Html.DisplayNameFor(model => model.Reincarcare[0].Client.NumeClient)            </th>            <th>                @Html.DisplayNameFor(model => model.Reincarcare[0].OferteReincarcare)            </th>            <th></th>        </tr>    </thead>    <tbody>        @foreach (var item in Model.ReincarcareD.Reincarcari)        {        <tr>            <td>                @Html.DisplayFor(modelItem => item.Numar)            </td>            <td>                @Html.DisplayFor(modelItem => item.Data)            </td>            <td>                @Html.DisplayFor(modelItem => item.Client.NumeClient)            </td>            <td>                @{                    foreach (var oferta in item.OferteReincarcare)                    {                        @oferta.Oferta.ID @: @oferta.Oferta.NumeOferta <br />                    }                }            </td>            <td>                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>            </td>        </tr>        }    </tbody></table>